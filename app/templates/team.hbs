<div class="main_wrap">
    <header></header>
    <div class="content_wrap">
        <div class="content">
            <h1>Team</h1>
            {{#each user in controller.model}}
                <div class="usr">
                    <div class="info"><svg height="100" width="100"><image clip-path="url(#hex)" {{bind-attr xlink:href=user.avatar_url}} height="100" width="100"></image></svg>
                        <div><h3>{{user.login}}</h3>
                        <span><a {{bind-attr href=user.github}} target="_blank"> {{user.github}}</a></span></div></div>
                </div><!--usr-->
            {{/each}}

        </div><!--content-->
        <aside></aside>
    </div><!--content_wrap-->
</div> <!--main_wrap-->